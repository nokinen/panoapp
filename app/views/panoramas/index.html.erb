<div id="previews">
<% @panoramas.each do |panorama| %>
<div class="preview">
	<%= link_to panorama do %>
		<%= image_tag panorama.preview %>
	<% end %>
</div>
<% end %>
</div>
<div id="upload">
	<% if mobile_device? %>
		<input id="files" type="file" name="files[]" accept="image/jpeg" multiple>
	<% else %>
		<div id="dropzone">+</div>
	<% end %>
</div>